@model MovieBox.ViewModels.RandomMovieViewModel;
@{
    ViewData["Title"] = "Home Page";
}

<h4>@Model.Movie.Name</h4>
@if (Model.Customers.Count == 5)
{
    <text>No one has rented this movie</text>
}
else
{
    <ul>
        @foreach (var customer in Model.Customers)
        {
            <li>@customer.Name</li>
        }
    </ul>
}